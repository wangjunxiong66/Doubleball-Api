<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wjx.sportsplatformapi.dao.PoemDao"> <!--namespace对应是哪个dao文件，该文件下的操作对应此map文件的sql操作-->

    <insert id="addPoem">
        insert into poem(title,author,dynasty,content,comment,create_time,update_time) values(#{title},#{author},#{dynasty},#{content},#{comment},#{create_time},#{update_time}) ;
    </insert>


</mapper>
